<DModal
  @title={{i18n "admin.logs.staff_actions.modal_title"}}
  @closeModal={{@closeModal}}
  @bodyClass="theme-change-modal-body"
  class="history-modal"
>
  <:body>
    {{html-safe this.diff}}
  </:body>
  <:footer>
    <DButton class="btn-primary" @action={{@closeModal}} @label="close" />
  </:footer>
</DModal>